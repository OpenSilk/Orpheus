<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OrpheusStyleables">
        <attr name="fabPlayStyle" format="reference"/>
        <attr name="fabNextStyle" format="reference"/>
        <attr name="fabPrevStyle" format="reference"/>
        <attr name="fabShuffleStyle" format="reference"/>
        <attr name="fabRepeatStyle" format="reference"/>
        <attr name="gridTileDescriptionStyle" format="reference"/>
        <attr name="gridTileDescriptionTitleStyle" format="reference"/>
        <attr name="gridTileDescriptionSubTitleStyle" format="reference"/>
        <attr name="nowPlayingPlayStyle" format="reference"/>
        <attr name="nowPlayingNextStyle" format="reference"/>
        <attr name="nowPlayingPrevStyle" format="reference"/>
        <attr name="nowPlayingShuffleStyle" format="reference"/>
        <attr name="nowPlayingRepeatStyle" format="reference"/>
        <attr name="nowPlayingCardBackground" format="reference"/>
        <attr name="controlsPlayStyle" format="reference"/>
        <attr name="controlsNextStyle" format="reference"/>
        <attr name="controlsPrevStyle" format="reference"/>
        <attr name="controlsShuffleStyle" format="reference"/>
        <attr name="controlsRepeatStyle" format="reference"/>
        <attr name="mainContentTopMargin" format="reference"/>
        <attr name="footerPlayStyle" format="reference"/>
        <attr name="iconTint" format="reference"/>
